def get_second_word_in_uppercase(sentence):
    # Split the sentence into words
    words = sentence.split()
    
    # Check if there are at least two words
    if len(words) >= 2:
        # Return the second word in uppercase
        return words[1].upper()
    else:
        # If there are less than two words, return "LESS"
        return "LESS"

# Take input from the user
sentence = input()

# Get the second word in uppercase
result = get_second_word_in_uppercase(sentence)

# Print the result
print( result)
