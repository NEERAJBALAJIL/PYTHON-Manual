def get_factor(n, p):
    factors = []
    for i in range(1, int(n**0.5) + 1):
        if n % i == 0:
            factors.append(i)
            if i != n // i:
                factors.append(n // i)
    factors.sort()
    if p > len(factors):
        return 0
    else:
        return factors[p - 1]

# Input
n = int(input())
p = int(input())

# Output
print(get_factor(n, p))
